store() {
  src="$srcdir/$1"
  dst="$storedir/$2"

  [[ -f "$src" || -d "$src" ]] || die "src does not exist [$src]"

  mkdir -p "$(dirname $dst)"

  echo "$src --> $dst"
  case $type in
    bin)
      install -Dm 0755 "$src" "$dst"
      ;;
    *)
      install -Dm 0644 "$src" "$dst"
      ;;
  esac
}

# vim: ft=bash
